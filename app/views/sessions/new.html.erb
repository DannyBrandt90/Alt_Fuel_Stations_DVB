<div id="card_container">

<h1>Login</h1>

<%= form_for :user, :url => login_user_path do |f|%>
    <%= f.label :email %><br>
    <%= f.email_field :email %><br>

    <%= f.label :password %><br>
    <%= f.password_field :password %><br>

    <%= f.submit "Sign In"%>
<%end%>

<%= button_to "Login with GitHub", '/auth/github' %>

</div>


